<%include head%>

<body>

	<div class="container">
		<div class="row">
			<div class="col-sm-12 movie">
				<h1>
					<%=currentMovie.title%>
				</h1>
				<img src="<%=imageBaseUrl+currentMovie.poster_path%>" />
				<h2>
					Genre(s): <% for(let i = 0; i < 4; i++){%>

						<%=currentMovie.genre[i]%>

					<% } %>
				</h2>
				<h3>

				</h3>
				<p>
					<%=currentMovie.overview%>
				</p>
			</div>
		</div>
	</div>

</body>
</html>